code:unitConversion
  rdf:type spin:Function ;
  spin:body [
      rdf:type sp:Select ;
      sp:resultVariables (
          [
            sp:varName "normResult" ;
          ]
        ) ;
      sp:where (
          [
            sp:object [
                sp:varName "orresu" ;
              ] ;
            sp:predicate code:hasUnit ;
            sp:subject [
                sp:varName "origResult" ;
              ] ;
          ]
          [
            sp:object [
                sp:varName "orres" ;
              ] ;
            sp:predicate code:hasValue ;
            sp:subject [
                sp:varName "origResult" ;
              ] ;
          ]
          [
            sp:object [
                sp:varName "orresu" ;
              ] ;
            sp:predicate code:hasOriginalUnit ;
            sp:subject [
                sp:varName "convfactor" ;
              ] ;
          ]
          [
            rdf:type sp:Optional ;
            sp:elements (
                [
                  sp:object [
                      sp:varName "cfn" ;
                    ] ;
                  sp:predicate code:hasConversionFactor ;
                  sp:subject [
                      sp:varName "convfactor" ;
                    ] ;
                ]
              ) ;
          ]
          [
            rdf:type sp:Bind ;
            sp:expression [
                rdf:type sp:if ;
                sp:arg1 [
                    rdf:type sp:eq ;
                    sp:arg1 [
                        sp:varName "orresu" ;
                      ] ;
                    sp:arg2 code:Unit_F ;
                  ] ;
                sp:arg2 [
                    rdf:type spif:decimalFormat ;
                    sp:arg1 [
                        rdf:type sp:mul ;
                        sp:arg1 [
                            rdf:type sp:sub ;
                            sp:arg1 [
                                sp:varName "orres" ;
                              ] ;
                            sp:arg2 32 ;
                          ] ;
                        sp:arg2 [
                            rdf:type sp:divide ;
                            sp:arg1 5 ;
                            sp:arg2 9 ;
                          ] ;
                      ] ;
                    sp:arg2 "#.#" ;
                  ] ;
                sp:arg3 [
                    rdf:type spif:decimalFormat ;
                    sp:arg1 [
                        rdf:type sp:mul ;
                        sp:arg1 [
                            sp:varName "orres" ;
                          ] ;
                        sp:arg2 [
                            sp:varName "cfn" ;
                          ] ;
                      ] ;
                    sp:arg2 "#" ;
                  ] ;
              ] ;
            sp:variable [
                sp:varName "normResult" ;
              ] ;
          ]
        ) ;
    ] ;
  spin:constraint [
      rdf:type spl:Argument ;
      spl:predicate arg:origResult ;
      spl:valueType rdfs:Resource ;
    ] ;
  rdfs:subClassOf spin:Functions ;
  skos:prefLabel "Unit conversion" ;
.
code:unitConversionChar
  rdf:type spin:Function ;
  spin:body [
      rdf:type sp:Select ;
      sp:resultVariables (
          [
            sp:varName "normResultChar" ;
          ]
        ) ;
      sp:where (
          [
            rdf:type sp:Bind ;
            sp:expression [
                rdf:type code:unitConversion ;
                arg:origResult [
                    sp:varName "origResult" ;
                  ] ;
              ] ;
            sp:variable [
                sp:varName "normResult" ;
              ] ;
          ]
          [
            rdf:type sp:Bind ;
            sp:expression [
                rdf:type xsd:string ;
                sp:arg1 [
                    sp:varName "normResult" ;
                  ] ;
              ] ;
            sp:variable [
                sp:varName "normResultChar" ;
              ] ;
          ]
        ) ;
    ] ;
  spin:constraint [
      rdf:type spl:Argument ;
      spl:predicate arg:origResult ;
      spl:valueType rdfs:Resource ;
    ] ;
  rdfs:subClassOf spin:Functions ;
  skos:prefLabel "Unit conversion" ;
.
code:unitNormalized
  rdf:type spin:Function ;
  spin:body [
      rdf:type sp:Select ;
      sp:resultVariables (
          [
            sp:varName "normUnit" ;
          ]
        ) ;
      sp:where (
          [
            sp:object [
                sp:varName "orresu" ;
              ] ;
            sp:predicate code:hasUnit ;
            sp:subject [
                sp:varName "origResult" ;
              ] ;
          ]
          [
            sp:object [
                sp:varName "orresu" ;
              ] ;
            sp:predicate code:hasOriginalUnit ;
            sp:subject [
                sp:varName "convfactor" ;
              ] ;
          ]
          [
            sp:object [
                sp:varName "sresu" ;
              ] ;
            sp:predicate code:hasStandardUnit ;
            sp:subject [
                sp:varName "convfactor" ;
              ] ;
          ]
          [
            sp:object [
                sp:varName "normUnit" ;
              ] ;
            sp:predicate skos:prefLabel ;
            sp:subject [
                sp:varName "sresu" ;
              ] ;
          ]
        ) ;
    ] ;
  spin:constraint [
      rdf:type spl:Argument ;
      spl:predicate arg:origResult ;
      spl:valueType rdfs:Resource ;
    ] ;
  rdfs:subClassOf spin:Functions ;
  skos:prefLabel "unit normalized" ;
