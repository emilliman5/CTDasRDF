study:ProductAdministrationInterval
  rdf:type owl:Class ;
  spin:rule [
      rdf:type sp:Construct ;
      sp:templates (
          [
            sp:object [
                sp:varName "d1"^^xsd:string ;
              ] ;
            sp:predicate time:hasBeginning ;
            sp:subject spin:_this ;
          ]
          [
            sp:object [
                sp:varName "d2"^^xsd:string ;
              ] ;
            sp:predicate time:hasEnd ;
            sp:subject spin:_this ;
          ]
        ) ;
      sp:where (
          [
            rdf:type sp:SubQuery ;
            sp:query [
                rdf:type sp:Select ;
                sp:groupBy (
                    [
                      sp:varName "s"^^xsd:string ;
                    ]
                  ) ;
                sp:resultVariables (
                    [
                      sp:varName "s"^^xsd:string ;
                    ]
                    [
                      sp:expression [
                          rdf:type sp:Min ;
                          sp:expression [
                              sp:varName "startdate"^^xsd:string ;
                            ] ;
                        ] ;
                      sp:varName "refstartdate"^^xsd:string ;
                    ]
                    [
                      sp:expression [
                          rdf:type sp:Max ;
                          sp:expression [
                              sp:varName "enddate"^^xsd:string ;
                            ] ;
                        ] ;
                      sp:varName "refenddate"^^xsd:string ;
                    ]
                  ) ;
                sp:where (
                    [
                      sp:object [
                          sp:varName "visit"^^xsd:string ;
                        ] ;
                      sp:predicate study:participatesIn ;
                      sp:subject [
                          sp:varName "s"^^xsd:string ;
                        ] ;
                    ]
                    [
                      sp:object study:Visit ;
                      sp:predicate rdf:type ;
                      sp:subject [
                          sp:varName "visit"^^xsd:string ;
                        ] ;
                    ]
                    [
                      sp:object [
                          sp:varName "act"^^xsd:string ;
                        ] ;
                      sp:predicate study:hasSubActivity ;
                      sp:subject [
                          sp:varName "visit"^^xsd:string ;
                        ] ;
                    ]
                    [
                      sp:object study:FixedDoseDrugAdministration ;
                      sp:predicate rdf:type ;
                      sp:subject [
                          sp:varName "act"^^xsd:string ;
                        ] ;
                    ]
                    [
                      sp:object [
                          sp:varName "acti"^^xsd:string ;
                        ] ;
                      sp:predicate study:hasInterval ;
                      sp:subject [
                          sp:varName "act"^^xsd:string ;
                        ] ;
                    ]
                    [
                      sp:object study:FixedDoseInterval ;
                      sp:predicate rdf:type ;
                      sp:subject [
                          sp:varName "acti"^^xsd:string ;
                        ] ;
                    ]
                    [
                      sp:object [
                          sp:varName "start"^^xsd:string ;
                        ] ;
                      sp:predicate time:hasBeginning ;
                      sp:subject [
                          sp:varName "acti"^^xsd:string ;
                        ] ;
                    ]
                    [
                      sp:object [
                          sp:varName "end"^^xsd:string ;
                        ] ;
                      sp:predicate time:hasEnd ;
                      sp:subject [
                          sp:varName "acti"^^xsd:string ;
                        ] ;
                    ]
                    [
                      sp:object [
                          sp:varName "startstring"^^xsd:string ;
                        ] ;
                      sp:predicate study:dateTimeInXSDString ;
                      sp:subject [
                          sp:varName "start"^^xsd:string ;
                        ] ;
                    ]
                    [
                      rdf:type sp:Bind ;
                      sp:expression [
                          rdf:type study:dateiso ;
                          arg:datestring [
                              sp:varName "startstring"^^xsd:string ;
                            ] ;
                        ] ;
                      sp:variable [
                          sp:varName "startdate"^^xsd:string ;
                        ] ;
                    ]
                    [
                      sp:object [
                          sp:varName "endstring"^^xsd:string ;
                        ] ;
                      sp:predicate study:dateTimeInXSDString ;
                      sp:subject [
                          sp:varName "end"^^xsd:string ;
                        ] ;
                    ]
                    [
                      rdf:type sp:Bind ;
                      sp:expression [
                          rdf:type study:dateiso ;
                          arg:datestring [
                              sp:varName "endstring"^^xsd:string ;
                            ] ;
                        ] ;
                      sp:variable [
                          sp:varName "enddate"^^xsd:string ;
                        ] ;
                    ]
                  ) ;
              ] ;
          ]
          [
            sp:object [
                sp:varName "padmin"^^xsd:string ;
              ] ;
            sp:predicate study:participatesIn ;
            sp:subject [
                sp:varName "s"^^xsd:string ;
              ] ;
          ]
          [
            sp:object study:ProductAdministration ;
            sp:predicate rdf:type ;
            sp:subject [
                sp:varName "padmin"^^xsd:string ;
              ] ;
          ]
          [
            sp:object spin:_this ;
            sp:predicate study:hasInterval ;
            sp:subject [
                sp:varName "padmin"^^xsd:string ;
              ] ;
          ]
          [
            sp:object study:ProductAdministrationInterval ;
            sp:predicate rdf:type ;
            sp:subject spin:_this ;
          ]
          [
            rdf:type sp:Bind ;
            sp:expression [
                rdf:type xsd:string ;
                sp:arg1 [
                    sp:varName "refstartdate"^^xsd:string ;
                  ] ;
              ] ;
            sp:variable [
                sp:varName "refstartdatestring"^^xsd:string ;
              ] ;
          ]
          [
            rdf:type sp:Bind ;
            sp:expression [
                rdf:type xsd:string ;
                sp:arg1 [
                    sp:varName "refenddate"^^xsd:string ;
                  ] ;
              ] ;
            sp:variable [
                sp:varName "refenddatestring"^^xsd:string ;
              ] ;
          ]
          [
            sp:object [
                sp:varName "refstartdatestring"^^xsd:string ;
              ] ;
            sp:predicate study:dateTimeInXSDString ;
            sp:subject [
                sp:varName "d1"^^xsd:string ;
              ] ;
          ]
          [
            sp:object [
                sp:varName "refenddatestring"^^xsd:string ;
              ] ;
            sp:predicate study:dateTimeInXSDString ;
            sp:subject [
                sp:varName "d2"^^xsd:string ;
              ] ;
          ]
        ) ;
      rdfs:comment "Calculate Product Administration Start and End dates from individual exposure data"^^xsd:string ;
    ] ;
  rdfs:comment ""^^xsd:string ;
  rdfs:label "Product administration interval"^^xsd:string ;
  rdfs:subClassOf study:ActivityInterval ;
  skos:definition ""^^xsd:string ;
