  spin:constraint [
      rdf:type sp:Ask ;
      sp:where (
          [
            sp:object study:AgeOutcome ;
            sp:predicate rdf:type ;
            sp:subject spin:_this ;
          ]
          [
            sp:object [
                sp:varName "age"^^xsd:string ;
              ] ;
            sp:predicate code:hasValue ;
            sp:subject spin:_this ;
          ]
          [
            rdf:type sp:Filter ;
            sp:expression [
                rdf:type sp:lt ;
                sp:arg1 [
                    sp:varName "age"^^xsd:string ;
                  ] ;
                sp:arg2 0 ;
              ] ;
          ]
        ) ;
    ] ;
