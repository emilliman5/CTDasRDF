study:AgeOutcome
  rdf:type owl:Class ;
  spin:constraint [
      rdf:type sp:Ask ;
      sp:where (
          [
            sp:object study:AgeOutcome ;
            sp:predicate rdf:type ;
            sp:subject spin:_this ;
          ]
          [
            sp:object [
                sp:varName "age" ;
              ] ;
            sp:predicate code:hasValue ;
            sp:subject spin:_this ;
          ]
          [
            rdf:type sp:Filter ;
            sp:expression [
                rdf:type sp:lt ;
                sp:arg1 [
                    sp:varName "age" ;
                  ] ;
                sp:arg2 0 ;
              ] ;
          ]
        ) ;
    ] ;
  rdfs:subClassOf study:NumericOutcome ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom time:TemporalUnit ;
      owl:onProperty code:hasUnit ;
    ] ;
  skos:altLabel "" ;
  skos:prefLabel "Age" ;
