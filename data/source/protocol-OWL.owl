###  https://w3id.org/phuse/cd01p#StartRuleLying5
cd01p:StartRuleLying5 rdf:type owl:Class ;
                      rdfs:subClassOf study:StartRulePrereqActivityOutcome ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty study:hasPrerequisite ;
                                        owl:allValuesFrom custom:AssumeBodyPositionSupine
                                      ] ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty study:hasPrereqExpectedOutcome ;
                                        owl:hasValue code:BodyPosition_SUPINE
                                      ] ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty skos:altLabel ;
                                        owl:hasValue "815"
                                      ] ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty skos:definition ;
                                        owl:hasValue "Target activity starts after the subject has assumed a supine position for 5 minutes"
                                      ] ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty skos:prefLabel ;
                                        owl:hasValue "Rule Lying 5 Min"
                                      ] ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty study:hasDelay ;
                                        owl:hasValue "PT5M"^^xsd:duration
                                      ] ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty study:ruleDescription ;
                                        owl:hasValue "AFTER LYING DOWN FOR 5 MINUTES"
                                      ] ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty study:ruleDescriptionLong ;
                                        owl:hasValue "Activity takes place 5 minutes after assuming a supine body position"
                                      ] ;
                      rdfs:comment "" ;
                      skos:altLabel "815" ;
                      skos:definition "Target activity starts after the subject has assumed a supine position for 5 minutes" ;
                      skos:prefLabel "Rule Lying 5 Min" ;
                      study:hasPrereqExpectedOutcome code:BodyPosition_SUPINE ;
                      study:hasPrereqExpectedStatus code:ActivityStatus_CO ;
                      study:hasPrerequisite custom:AssumeBodyPositionSupine ;
                      study:ruleDescription "AFTER LYING DOWN FOR 5 MINUTES" ;
                      study:ruleDescriptionLong "Activity takes place 5 minutes after assuming a supine body position" .


