# get all within a specified path
SELECT DISTINCT *
  WHERE {
      cdiscpilot01:Person_01-701-1015 ?p ?o .
      BIND("cdiscpilot01:Person_01-701-1015" as ?s)
      OPTIONAL{
          ?o ?p2 ?o2 .
      }
      OPTIONAL{
          ?o2 ?p3 ?o3 .
      }
  }


#Delete all tripples
DELETE{?s ?p ?o} WHERE{?s ?p ?o}


SELECT * WHERE {?s  ?p ?o}

#get all types
SELECT DISTINCT ?t WHERE {?s rdf:type ?t}

#get all classes
SELECT DISTINCT ?s WHERE {?s rdf:type owl:Class}

# investigate all types / classes?
select distinct ?type where { { ?ind a ?type . }} ORDER BY ?type


SELECT * WHERE {?s  study:hasSite ?o}

SELECT * WHERE {cd01p:Site_701  ?p ?o}

SELECT * WHERE {study:EnrolledSubject ?p ?o}
SELECT * WHERE {?s ?p study:EnrolledSubject}

# get all OWLs and import them
SELECT * WHERE {?s owl:imports ?o}

